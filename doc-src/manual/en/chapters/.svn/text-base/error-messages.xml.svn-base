                    <title>English error messages</title>
                    <tgroup cols="2">
                         <thead>
                              <row>
                                   <entry> Error code </entry>
                                   <entry> Error message </entry>
                              </row>
                         </thead>
                         <tbody>
                              <row>
                                   <entry>
                                        <code>1</code>
                                   </entry>
